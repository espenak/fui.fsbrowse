<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    i18n_domain="fui.fsbrowse">

    <class class=".filesystemfolder.FilesystemFolder">
        <require
            permission="zope2.View"
            interface="..interfaces.IFilesystemFolder"
            />
        <require
            permission="cmf.ModifyPortalContent"
            set_schema="..interfaces.IFilesystemFolder"
            />
    </class>
</configure>
